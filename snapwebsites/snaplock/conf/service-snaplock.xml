<service name="snaplock" required="required">
  <priority>-10</priority>
  <config>/etc/snapwebsites/snaplock.conf</config>
  <recovery>60</recovery>
  <user>snapwebsites</user>
  <group>snapwebsites</group>
  <dependencies>
    <dependency>snapinit</dependency>
  </dependencies>
</service>
